{
    "manifest_version":2,
    "name":"My Extension",
    "version":"0.2",
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":["content.js"]
        }
    ],
    "icons": { "48": "icon.png" },  
    "background": {  
        "scripts": ["background.js"]  
    },
    "browser_action": {
        "default_icon": "icon.png"
    }
}